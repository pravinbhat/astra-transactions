<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %magenta(%-5level) %cyan(%-25logger): %msg%n</pattern>
		</encoder>
	</appender>
	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
	<logger name="com.bhatman.astra" level="INFO" />
	<logger name="com.datastax.oss" level="WARN" />
</configuration>